{
  "name": "@ldesign/template-core",
  "version": "1.0.0",
  "description": "核心模板管理系统 - 框架无关",
  "keywords": [
    "template",
    "template-engine",
    "registry",
    "template-management"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.cjs",
      "default": "./esm/index.js"
    },
    "./locales": {
      "types": "./esm/locales/index.d.ts",
      "import": "./esm/locales/index.js",
      "require": "./lib/locales/index.cjs",
      "default": "./esm/locales/index.js"
    },
    "./locales/*": {
      "types": "./esm/locales/*.d.ts",
      "import": "./esm/locales/*.js",
      "require": "./lib/locales/*.cjs",
      "default": "./esm/locales/*.js"
    },
    "./manager": {
      "types": "./esm/manager/index.d.ts",
      "import": "./esm/manager/index.js",
      "require": "./lib/manager/index.cjs",
      "default": "./esm/manager/index.js"
    },
    "./manager/*": {
      "types": "./esm/manager/*.d.ts",
      "import": "./esm/manager/*.js",
      "require": "./lib/manager/*.cjs",
      "default": "./esm/manager/*.js"
    },
    "./query": {
      "types": "./esm/query/index.d.ts",
      "import": "./esm/query/index.js",
      "require": "./lib/query/index.cjs",
      "default": "./esm/query/index.js"
    },
    "./query/*": {
      "types": "./esm/query/*.d.ts",
      "import": "./esm/query/*.js",
      "require": "./lib/query/*.cjs",
      "default": "./esm/query/*.js"
    },
    "./registry": {
      "types": "./esm/registry/index.d.ts",
      "import": "./esm/registry/index.js",
      "require": "./lib/registry/index.cjs",
      "default": "./esm/registry/index.js"
    },
    "./registry/*": {
      "types": "./esm/registry/*.d.ts",
      "import": "./esm/registry/*.js",
      "require": "./lib/registry/*.cjs",
      "default": "./esm/registry/*.js"
    },
    "./types": {
      "types": "./esm/types/index.d.ts",
      "import": "./esm/types/index.js",
      "require": "./lib/types/index.cjs",
      "default": "./esm/types/index.js"
    },
    "./types/*": {
      "types": "./esm/types/*.d.ts",
      "import": "./esm/types/*.js",
      "require": "./lib/types/*.cjs",
      "default": "./esm/types/*.js"
    },
    "./utils": {
      "types": "./esm/utils/index.d.ts",
      "import": "./esm/utils/index.js",
      "require": "./lib/utils/index.cjs",
      "default": "./esm/utils/index.js"
    },
    "./utils/*": {
      "types": "./esm/utils/*.d.ts",
      "import": "./esm/utils/*.js",
      "require": "./lib/utils/*.cjs",
      "default": "./esm/utils/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "esm",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "typescript": "^5.7.3"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}