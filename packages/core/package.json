{
  "name": "@ldesign/template-core",
  "version": "1.0.0",
  "description": "核心模板管理系统 - 框架无关",
  "keywords": [
    "template",
    "template-engine",
    "registry",
    "template-management"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs",
      "default": "./es/index.js"
    },
    "./manager": {
      "types": "./es/manager/index.d.ts",
      "import": "./es/manager/index.js",
      "require": "./lib/manager/index.cjs",
      "default": "./es/manager/index.js"
    },
    "./manager/*": {
      "types": "./es/manager/*.d.ts",
      "import": "./es/manager/*.js",
      "require": "./lib/manager/*.cjs",
      "default": "./es/manager/*.js"
    },
    "./query": {
      "types": "./es/query/index.d.ts",
      "import": "./es/query/index.js",
      "require": "./lib/query/index.cjs",
      "default": "./es/query/index.js"
    },
    "./query/*": {
      "types": "./es/query/*.d.ts",
      "import": "./es/query/*.js",
      "require": "./lib/query/*.cjs",
      "default": "./es/query/*.js"
    },
    "./registry": {
      "types": "./es/registry/index.d.ts",
      "import": "./es/registry/index.js",
      "require": "./lib/registry/index.cjs",
      "default": "./es/registry/index.js"
    },
    "./registry/*": {
      "types": "./es/registry/*.d.ts",
      "import": "./es/registry/*.js",
      "require": "./lib/registry/*.cjs",
      "default": "./es/registry/*.js"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs",
      "default": "./es/types/index.js"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs",
      "default": "./es/types/*.js"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs",
      "default": "./es/utils/index.js"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs",
      "default": "./es/utils/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "typescript": "^5.7.3"
  },
  "typings": "./es/index.d.ts",
  "browser": "./dist/index.js"
}