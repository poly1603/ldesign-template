    "./lib/templates/layout/desktop/dual-column/index.vue2.css": "./lib/templates/layout/desktop/dual-column/index.vue2.css",
    "./lib/templates/layout/desktop/mix/index.vue.css": "./lib/templates/layout/desktop/mix/index.vue.css",
    "./lib/templates/layout/desktop/sidebar/index.vue.css": "./lib/templates/layout/desktop/sidebar/index.vue.css",
    "./lib/templates/layout/desktop/top-menu/index.vue.css": "./lib/templates/layout/desktop/top-menu/index.vue.css",
    "./lib/templates/layout/mobile/drawer/index.vue.css": "./lib/templates/layout/mobile/drawer/index.vue.css",
    "./lib/templates/layout/mobile/tab-bar/index.vue.css": "./lib/templates/layout/mobile/tab-bar/index.vue.css",
    "./lib/templates/layout/mobile/top-menu/index.vue.css": "./lib/templates/layout/mobile/top-menu/index.vue.css",
    "./lib/templates/layout/tablet/drawer/index.vue.css": "./lib/templates/layout/tablet/drawer/index.vue.css",
    "./lib/templates/layout/tablet/sidebar/index.vue.css": "./lib/templates/layout/tablet/sidebar/index.vue.css",
    "./lib/templates/layout/tablet/top-menu/index.vue.css": "./lib/templates/layout/tablet/top-menu/index.vue.css",
    "./lib/templates/login/desktop/default/index.vue.css": "./lib/templates/login/desktop/default/index.vue.css",
    "./lib/templates/login/desktop/minimal/index.vue.css": "./lib/templates/login/desktop/minimal/index.vue.css",
    "./lib/templates/login/mobile/default/index.vue.css": "./lib/templates/login/mobile/default/index.vue.css",
    "./lib/templates/login/mobile/minimal/index.vue.css": "./lib/templates/login/mobile/minimal/index.vue.css",
    "./lib/templates/login/tablet/default/index.vue.css": "./lib/templates/login/tablet/default/index.vue.css",
    "./lib/templates/login/tablet/minimal/index.vue.css": "./lib/templates/login/tablet/minimal/index.vue.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css",
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "esm",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder watch",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/template-core": "workspace:*",
    "@vueuse/core": "^13.7.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.7.3",
    "vue": "^3.4.0",
    "vue-tsc": "^3.0.0"
  },
  "typings": "./esm/index.d.ts",
  "browser": "./dist/index.js"
}