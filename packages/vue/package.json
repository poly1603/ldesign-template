{
  "name": "@ldesign/template-vue",
  "version": "1.0.0",
  "description": "Vue 3 模板管理系统 - 基于 @ldesign/template-core",
  "keywords": [
    "vue",
    "vue3",
    "template",
    "template-engine",
    "dynamic-rendering",
    "composition-api"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.cjs",
      "default": "./esm/index.js"
    },
    "./components": {
      "types": "./esm/components/index.d.ts",
      "import": "./esm/components/index.js",
      "require": "./lib/components/index.cjs",
      "default": "./esm/components/index.js"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./lib/components/*.cjs",
      "default": "./esm/components/*.js"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./lib/composables/index.cjs",
      "default": "./esm/composables/index.js"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./lib/composables/*.cjs",
      "default": "./esm/composables/*.js"
    },
    "./directives": {
      "types": "./esm/directives/index.d.ts",
