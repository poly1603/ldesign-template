      "types": "./esm/directives/*.d.ts",
      "import": "./esm/directives/*.js",
      "require": "./lib/directives/*.cjs",
      "default": "./esm/directives/*.js"
    },
    "./plugin": {
      "types": "./esm/plugin/index.d.ts",
      "import": "./esm/plugin/index.js",
      "require": "./lib/plugin/index.cjs",
      "default": "./esm/plugin/index.js"
    },
    "./plugin/*": {
      "types": "./esm/plugin/*.d.ts",
      "import": "./esm/plugin/*.js",
      "require": "./lib/plugin/*.cjs",
      "default": "./esm/plugin/*.js"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./lib/plugins/index.cjs",
      "default": "./esm/plugins/index.js"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./lib/plugins/*.cjs",
      "default": "./esm/plugins/*.js"
    },
    "./scanner": {
      "types": "./esm/scanner/index.d.ts",
      "import": "./esm/scanner/index.js",
      "require": "./lib/scanner/index.cjs",
      "default": "./esm/scanner/index.js"
    },
    "./scanner/*": {
      "types": "./esm/scanner/*.d.ts",
      "import": "./esm/scanner/*.js",
      "require": "./lib/scanner/*.cjs",
      "default": "./esm/scanner/*.js"
    },
    "./templates": {
      "types": "./esm/templates/index.d.ts",
      "import": "./esm/templates/index.js",
      "require": "./lib/templates/index.cjs",
      "default": "./esm/templates/index.js"
    },
    "./templates/*": {
      "types": "./esm/templates/*.d.ts",
      "import": "./esm/templates/*.js",
      "require": "./lib/templates/*.cjs",
      "default": "./esm/templates/*.js"
    },
    "./types/*": {
      "types": "./esm/types/*.d.ts",
      "import": "./esm/types/*.js",
      "require": "./lib/types/*.cjs",
      "default": "./esm/types/*.js"
    },
    "./lib/components/layout/LayoutContent.vue.css": "./lib/components/layout/LayoutContent.vue.css",
