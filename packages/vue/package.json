  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ldesign/template-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "template",
    "dynamic",
    "component"
  ],
  "author": "ldesign",
  "license": "MIT"
}
